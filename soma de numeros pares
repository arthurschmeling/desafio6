package javaapplication101;


public class JavaApplication101 {

 
    public static void main(String[] args) {
    
        int soma = 0;

        // Loop de 1 a 200
        for (int i = 1; i <= 200; i++) {
            if (i % 2 != 0) { // Verifica se o número é ímpar
                soma += i;
            }
        }

        System.out.println("A soma dos números ímpares de 1 a 200 é: " + soma);
    }
}
